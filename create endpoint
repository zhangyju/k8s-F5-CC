apiVersion: extensions/v1beta1
kind: Deployment
metadata:
  name: f5-vxlan-lingque-endpoint
  namespace: f5-vxlan-lingque
spec:
  replicas: 1
  template:
    metadata:
      labels:
        run: f5-vxlan-lingque-endpoint
    spec:
      containers:
      - image: "10.194.184.1:60080/f5networks/f5-demo-app"
        env:
        - name: F5DEMO_APP
          value: "frontend"
        - name: F5DEMO_BACKEND_URL
          value: "http://my-backend/"
        imagePullPolicy: IfNotPresent
        name: f5-vxlan-lingque-endpoint
        ports:
        - containerPort: 80
          protocol: TCP
